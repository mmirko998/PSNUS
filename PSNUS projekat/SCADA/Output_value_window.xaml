<Window x:Class="SCADA.Output_value_window"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SCADA"
        mc:Ignorable="d"
        Title="Change value" Height="161.25" Width="271.25" ResizeMode="NoResize">
    <Grid>
        <TextBox Name="txt_value" HorizontalAlignment="Left" Height="23" Margin="77,56,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="120"/>
        <Label Name="Output_info" Content="Output info" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Button Name="Cancel_btn" Content="Cancel" HorizontalAlignment="Left" Margin="40,89,0,0" VerticalAlignment="Top" Width="75" Click="Cancel_btn_Click"/>
        <Button Name="Confirm_btn" Content="Confirm" HorizontalAlignment="Left" Margin="159,89,0,0" VerticalAlignment="Top" Width="75" Click="Confirm_btn_Click"/>

    </Grid>
</Window>
