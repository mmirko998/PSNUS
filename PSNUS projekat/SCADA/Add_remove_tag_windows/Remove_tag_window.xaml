<Window x:Class="SCADA.Remove_tag_window"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SCADA"
        mc:Ignorable="d"
        Title="Remove tag" Height="200" Width="400" ResizeMode="NoResize">
    <Grid>
        <Button Content="Yes" HorizontalAlignment="Left" Margin="240,82,0,0" VerticalAlignment="Top" Width="80" Height="40" Click="Yes_Click" />
        <Button Content="No" HorizontalAlignment="Left" Margin="80,82,0,0" VerticalAlignment="Top" Width="80" Height="40" Click="No_Click" />
        <TextBox HorizontalAlignment="Center" Height="38" Margin="0,28,0,0" TextWrapping="Wrap" Text="Are you sure you want to remove selected tag?" VerticalAlignment="Top" Width="142"/>

    </Grid>
</Window>
